<script>
    // Assuming orderStatus is a string, make sure it is in uppercase for consistent comparison
    var orderStatus = '<%= product.status.toUpperCase() %>';

    function updateProgressBar() {
        var progressBarItems = document.querySelectorAll('#progressbar li');

        // Reset all styles to default
        progressBarItems.forEach(function (item) {
            item.classList.remove('active', 'text-muted');
        });

        // Apply styles based on order status
        if (orderStatus === 'PLACED') {
            progressBarItems[0].classList.add('active');
        } else if (orderStatus === 'SHIPPED') {
            progressBarItems[0].classList.add('active');
            progressBarItems[1].classList.add('active');
        } else if (orderStatus === 'DELIVERED') {
            progressBarItems[0].classList.add('active');
            progressBarItems[1].classList.add('active');
            progressBarItems[2].classList.add('active');
        } else {
            // Handle other order statuses as needed
        }
    }

    // Call the function to update the progress bar when the page loads
    updateProgressBar();
</script>
