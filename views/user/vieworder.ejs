    <div class="login-page bg-image pt-8 pb-8 pt-md-12 pb-md-12 pt-lg-17 pb-lg-17" style="background-image: url('assets/images/backgrounds/login-bg.jpg')">
        
        <div class="container">
            <div class="form-box">
                <div class="order-success">
                    <div class="success-icon">
                        <i class="icon-check-circle"></i>
                    </div>
                    
        
                    <!-- Display wallet history -->
                    <div class="order-details">
                    
                    
                        
                        <div class="card px-2">
                            <div class="card-header bg-white">
                                <div class="row justify-content-between">
                                    <div class="col">
                                        <p class="text-muted"> Order ID <span class="font-weight-bold text-dark"><%= order._id %></span></p>
                                        <p class="text-muted"> Place On <span class="font-weight-bold text-dark"><%= order.date.toLocaleDateString(); %></span> </p>
                                    </div>
                                    <div class="flex-col my-auto"></div>
                                </div>
                            </div>
                            <div class="card-body">
                                <div class="media flex-column flex-sm-row">
                                    <div class="media-body">
                                        <h5 class="bold"><%= product.name %></h5>
                                        <p class="text-muted"> Qt: <%= product.quantity %></p>
                                        <h4 class="mt-3 mb-4 bold"> <span class="mt-5">&#x20B9;</span> <%= product.totalprice %></h4>
                                        <p>Payment:<%= order.payment %></p>

                                        <h3>Address</h3>
                                        <%
                                                        let parsedAddress;
                                                        try {
                                                            // Correct the string representation to make it valid JSON
                                                            const correctedAddressString = order.address
                                                            .replace(/'/g, '"')  // Replace single quotes with double quotes
                                                            .replace(/(\w+):/g, '"$1":')  // Wrap keys with double quotes
                                                            .replace(/"_id":\s*new\s*ObjectId\("([^"]+)"\)/g, '"_id": "$1"');  // Replace new ObjectId() with the string value

                                                        

                                                            // Parse the corrected string as JSON
                                                            parsedAddress = JSON.parse(correctedAddressString);
                                                        } catch (error) {
                                                            console.error('Error parsing address:', error.message);
                                                            parsedAddress = {};
                                                        }
%>
                                        <p>
                                            <%= parsedAddress.name %><br>
                                            <%= parsedAddress.buildingname %> ,<%= parsedAddress.street %> ,<%= parsedAddress.city %>,<%= parsedAddress.pincode %><br>
                                            <%= parsedAddress.state %><br>
                                            <%= parsedAddress.mobile %><br>
                                            
                                        </p>
                                    </div>
                                    <img class="align-self-center img-fluid" src="images/<%= product.image %>" width="180" height="180">
                                </div>
                            </div>
                            <div class="row px-3">
                                <div class="col">
